<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
  <display-name>Booktown Servlet</display-name>
  <description>DAO Sample for ser422</description>
  
  <servlet>
    <servlet-name>Booktown</servlet-name>
    <servlet-class>edu.asupoly.ser422.servlets.BooktownServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>Booktown</servlet-name>
    <url-pattern>/booktown</url-pattern>
  </servlet-mapping>
</web-app>
